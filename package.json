{
  "name": "new_home_page",
  "version": "1.0.0",
  "scripts": {
    "clean": "rimraf ./dist",
    "prebuild": "npm run clean",
    "build": "parcel build src/html/index.pug",
    "start": "concurrently \"npm run watch\" \"npx parcel serve src/html/index.pug\"",
    "watch": "parcel watch src/html/index.pug",
    "lint": "stylelint src/style/*.css",
    "lh": "lighthousebot",
    "cy": "cypress open",
    "cy:run": "cypress run",
    "cy:verify": "cypress verify",
    "serve": "serve -l 3000 ./dist"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "autoprefixer": "^9.4.9",
    "browserslist": "^4.4.2",
    "caniuse-lite": "^1.0.30000939",
    "concurrently": "^4.1.0",
    "css-mqpacker": "^7.0.0",
    "cypress": "3.2.0",
    "lighthousebot": "git+https://github.com/GoogleChromeLabs/lighthousebot.git",
    "parcel-bundler": "^1.11.0",
    "parcel-plugin-static-files-copy": "^2.0.0",
    "parcel-plugin-sw-cache": "^0.2.7",
    "postcss-flexbugs-fixes": "^4.1.0",
    "pug": "^2.0.3",
    "sass": "^1.17.2",
    "serve": "10.1.2",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0"
  },
  "dependencies": {
    "normalize.css": "^8.0.1",
    "typeface-karla": "0.0.72"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "font-family-no-missing-generic-family-keyword": null
    }
  },
  "cache": {
    "inDev": true,
    "swDest": "service-worker.js",
    "strategy": "default"
  },
  "engines": {
    "yarn": "YARN NO LONGER USED - use npm instead."
  }
}
